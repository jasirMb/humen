                        <td>{{inc @index}}</td>
                        <td>
                          {{this._id}}
                        </td>

                        <td>Rs {{this.sales}}</td>


                        <td>Rs
                          {{this.profit}}
                        </td>

                        <td>{{this.count}}</td>
                      </tr>
                      {{/each}}


