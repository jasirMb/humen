    <div class="container login_page margin_bottom_50">
        <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                <h1 class="title-font text-center capital margin_bottom_50">login</h1>
                <div class="form-customer des-font">
                    {{#if blockedErr}}
                    <h4 class="text-danger">you have been blocked!!!!</h4>
                    {{/if}}
                    <form action="/login" method="post">
                        <div class="form-group">
                            <label for="exampleInputEmail1">E-mail*</label>
                            <input type="email" class="form-control form-account" name="userEmail" placeholder="Email*" id="email">
                        </div>
                        {{#if loginErr}}
                        <h4 class="error text-danger">invalid Email or password </h4>
                        {{/if}}
                        <div class="form-group">
                            <label for="exampleInputPassword1">Password*</label>
                            <input type="password" class="form-control form-account" name="Password"
                                placeholder="Password*">
                        </div>


                        <div class="flex space_top_bot_30">
                            <a href="#recover" id="RecoverPassword" class="link-default">Forgot Password?</a>
                        </div>

                        <div class="btn-button-group mg-top-30 mg-bottom-15">
                            <button type="submit" class="btn-nixx full-width number-font uppercase" onclick="fieldvalidation()">
                                Sign In
                            </button>
                        </div>
                    </form>
                </div>
            </div>
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                <h1 class="title-font text-center capital margin_bottom_50">register</h1>

                <div class="form-customer des-font">
                    <form action="/signup" method="post">
                        <div class="form-group">
                            <div>
                                {{#if confirmPassErr}}
                                <h1 class="text-danger">Passwords should be same !!!!!</h1>
                                {{/if}}
                                <label for="exampleInputEmail1">Name*</label>
                                <input type="text" class="form-control form-account" name="userName"
                                    placeholder="Name*" id="username">
                                <span class="float-right text-danger" id="userNameErr"></span>
                            </div>
                            <div>
                                <label for="exampleInputEmail1">Email*</label>
                                <input type="email" class="form-control form-account" name="userEmail"
                                    placeholder="Email*" id="email">
                                <span class="float-right text-danger" id="emailErr"></span>

                            </div>
                            {{#if signupErr}}
                            <h4 class="error text-danger">Email already registered Please login </h4>
                            {{/if}}
                            <div>
                                <label for="exampleInputEmail1">Phone*</label>
                                <input type="Number" class="form-control form-account" name="phoneNo" id="mobile"
                                    placeholder="PhoneNo*">
                                <span class="float-right text-danger" id="phoneErr"></span>

                            </div>
                            <div class="form-group">
                                <label for="exampleInputEmail1">Password*</label>
                                <input type="password" class="form-control form-account" name="Password" id="password"
                                    placeholder="Password*" required="">
                                <span class="float-right text-danger" id="passwordErr"></span>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputEmail1">Confirm Password*</label>
                                <input type="password" class="form-control form-account" name="Password2" id="password2"
                                    onkeyup="validate_password()" placeholder="Password*">
                                <span class="float-right text-danger" id="comPassErr"></span>
                            </div>
                            <span id="wrong_pass_alert"></span>


                            <div class="btn-button-group">
                                <button type="submit" value="submit" class="btn-nixx full-width number-font uppercase"
                                    onclick="fieldvalidation()">
                                    register
                                </button>
                            </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
